  file = {/home/nilomr/Zotero/storage/NEZ93H6L/ADA168819.pdf}
}

@article{hastings,
  title = {Long-Term Predator\textendash Prey Cycles Finally Achieved in the Lab},
  author = {Hastings, Alan},
  journal = {Nature},
  doi = {10.1038/d41586-019-03603-3},
  abstract = {A key question in ecology is what allows species to persist over time \textemdash{} particularly when there are pairs of species in which one is an exploiter and the other its victim. A long-standing theory attempts to answer this question by explaining how relative numbers of predators and their prey can cycle continuously\textsuperscript{{$<$}a href="\#ref-CR1"{$>$}1{$<$}/a{$>$}}. First, prey numbers would increase, giving the predator more food. The subsequent increase in predators would lead to a decline in prey. Predator numbers would then decline owing to a lack of food, restarting the cycle. However, it has proved unexpectedly challenging to demonstrate this type of persistent predator\textendash prey cycle in simple controlled systems in the laboratory. {$<$}a href="https://www.nature.com/articles/s41586-019-1857-0"{$>$}Writing in \emph{Nature}{$<$}/a{$>$}, Blasius \emph{et al.}\textsuperscript{{$<$}a href="\#ref-CR2"{$>$}2{$<$}/a{$>$}} report just such a demonstration, succeeding where almost 90 years of experimental work has failed.}
}

