@article{tchernichovski2021,
  title = {Balanced Imitation Sustains Song Culture in Zebra Finches},
  author = {Tchernichovski, Ofer and {Eisenberg-Edidin}, Sophie and Jarvis, Erich D.},
  year = {2021},
  month = may,
  journal = {Nat Commun},
  volume = {12},
  number = {1},
  pages = {2562},
  publisher = {{Nature Publishing Group}},
  issn = {2041-1723},
  doi = {10.1038/s41467-021-22852-3},
  urldate = {2023-04-14},
  abstract = {Songbirds acquire songs by imitation, as humans do speech. Although imitation should drive convergence within a group and divergence through drift between groups, zebra finch songs sustain high diversity within a colony, but mild variation across colonies. We investigated this phenomenon by analyzing vocal learning statistics in 160 tutor-pupil pairs from a large breeding colony. Song imitation is persistently accurate in some families, but poor in others. This is not attributed to genetic differences, as fostered pupils copied their tutors' songs as accurately or poorly as biological pupils. Rather, pupils of tutors with low song diversity make more improvisations compared to pupils of tutors with high song diversity. We suggest that a frequency dependent balanced imitation prevents extinction of rare song elements and overabundance of common ones, promoting repertoire diversity within groups, while constraining drift across groups, which together prevents the collapse of vocal culture into either complete uniformity or chaos.},
  copyright = {2021 The Author(s)},
  langid = {english},
  keywords = {Animal behaviour,Behavioural ecology,Evolution of language,Psychology},
  file = {/home/nilomr/Zotero/storage/SNHNDRJX/Tchernichovski et al. - 2021 - Balanced imitation sustains song culture in zebra .pdf}
}

