  author = {Wang, Yan-hui and Engel, Michael S. and Rafael, Jos{\'e} A. and Wu, Hao-yang and R{\'e}dei, D{\'a}vid and Xie, Qiang and Wang, Gang and Liu, Xiao-guang and Bu, Wen-jun},
