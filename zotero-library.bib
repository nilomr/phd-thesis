  keywords = {Adult,Female,Humans,Life Style,Odds Ratio,Premenstrual Syndrome,Premenstrual Syndrome: epidemiology,Premenstrual Syndrome: etiology,Premenstrual Syndrome: physiopathology,Premenstrual Syndrome: psychology,Prevalence,Risk,Risk Factors,Virginia,Virginia: epidemiology},
