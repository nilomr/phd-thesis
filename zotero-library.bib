@article{clifford2004,
  title = {Mitochondrial {{DNA}} Phylogeography of Western Lowland Gorillas ({{Gorilla}} Gorilla Gorilla)},
  author = {Clifford, Stephen L. and Anthony, Nicola M. and {Bawe-Johnson}, Mireille and Abernethy, Kate A. and Tutin, Caroline E.G. and White, Lee J.T. and Bermejo, Magdelena and Goldsmith, Michelle L. and Mcfarland, Kelley and Jeffery, Kathryn J. and Bruford, Michael W. and Wickings, E. Jean},
  year = {2004},
