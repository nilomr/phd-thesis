@article{sallam2018,
  title = {New {{Egyptian}} Sauropod Reveals {{Late Cretaceous}} Dinosaur Dispersal between {{Europe}} and {{Africa}}},
  author = {Sallam, Hesham M and Gorscak, Eric and O'Connor, Patrick M. and {El-Dawoudi}, Iman A and {El-Sayed}, Sanaa and Saber, Sara and Kora, Mahmoud A and Sertich, Joseph J.W. and Seiffert, Erik R and Lamanna, Matthew C},
  year = {2018},
