}

@article{Little2008a,
  title = {Symmetry and Sexual Dimorphism in Human Faces: {{Interrelated}} Preferences Suggest Both Signal Quality},
  author = {Little, Anthony C. and Jones, Benedict C. and DeBruine, Lisa M. and Feinberg, David R.},
  year = {2008},
