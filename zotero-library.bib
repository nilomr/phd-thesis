@article{tencate2007,
  title = {Biases in Signal Evolution: Learning Makes a Difference},
  author = {{ten Cate}, Carel and Rowe, Candy},
  year = {2007},
  journal = {Trends in Ecology and Evolution},
  volume = {22},
