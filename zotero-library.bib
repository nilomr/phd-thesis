@article{soydaakyol2013,
  title = {Anger in Women with Premenstrual Dysphoric Disorder: {{Its}} Relations with Premenstrual Dysphoric Disorder and Sociodemographic and Clinical Variables},
  author = {Soyda Akyol, Esra and Karakaya Arisoy, Emel {\"O}zge and {\c C}ayk{\"o}yl{\"u}, Ali},
