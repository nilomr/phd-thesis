@article{onate-casado2023,
  title = {Guess Who? {{Evaluating}} Individual Acoustic Monitoring for Males and Females of the {{Tawny Pipit}}, a Migratory Passerine Bird with a Simple Song},
  shorttitle = {Guess Who?},
  author = {{O{\~n}ate-Casado}, Javier and Porte{\v s}, Michal and Beran, V{\'a}clav and Petrusek, Adam and Petruskov{\'a}, Tereza},
  year = {2023},
