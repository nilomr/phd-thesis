  file = {/home/nilomr/Zotero/storage/6WKFTIK3/full-text.pdf}
}

@article{janssen2017a,
  title = {Finding {{Occurrences}} of {{Melodic Segments}} in {{Folk Songs Employing Symbolic Similarity Measures}}},
