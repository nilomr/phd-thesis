@article{Mendenhall2017,
  title = {Non-Communicable Disease Syndemics: Poverty, Depression, and Diabetes among Low-Income Populations},
  author = {Mendenhall, Emily and Kohrt, Brandon A and Norris, Shane A and Ndetei, David and Prabhakaran, Dorairaj},
  year = {2017},
