@article{vawter2004,
  title = {Gender-{{Specific Gene Expression}} in {{Post-Mortem Human Brain}}: {{Localization}} to {{Sex Chromosomes}}},
  author = {Vawter, Marquis P and Evans, Simon and Choudary, Prabhakara and Tomita, Hiroaki and {Meador-Woodruff}, Jim and Molnar, Margherita and Li, Jun and Lopez, Juan F and Myers, Rick and Cox, David and Watson, Stanley J and Akil, Huda and Jones, Edward G and Bunney, William E},
  year = {2004},
