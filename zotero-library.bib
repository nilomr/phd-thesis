@article{brakes2021,
  title = {A Deepening Understanding of Animal Culture Suggests Lessons for Conservation},
  author = {Brakes, Philippa and Carroll, Emma L. and Dall, Sasha R. X. and Keith, Sally A. and McGregor, Peter K. and Mesnick, Sarah L. and Noad, Michael J. and Rendell, Luke and Robbins, Martha M. and Rutz, Christian and Thornton, Alex and Whiten, Andrew and Whiting, Martin J. and Aplin, Lucy M. and Bearhop, Stuart and Ciucci, Paolo and Fishlock, Vicki and Ford, John K. B. and {Notarbartolo di Sciara}, Giuseppe and Simmonds, Mark P. and Spina, Fernando and Wade, Paul R. and Whitehead, Hal and Williams, James and Garland, Ellen C.},
  year = {2021},
