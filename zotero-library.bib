@article{serafine1984,
  title = {Integration of Melody and Text in Memory for Songs},
  author = {Serafine, Mary Louise and Crowder, Robert G. and Repp, Bruno H.},
  year = {1984},
  month = apr,
  journal = {Cognition},
  volume = {16},
  number = {3},
  pages = {285--303},
  publisher = {{Elsevier}},
  issn = {0010-0277},
  doi = {10.1016/0010-0277(84)90031-3},
  urldate = {2018-07-04},
  abstract = {Two experiments examined whether the memory representation for songs consists of independent or integrated components (melody and text). Subjects heard a serial presentation of excerpts from largely unfamiliar folksongs, followed by a recognition test. The test required subjects to recognize songs, melodies, or texts and consisted of five types of items: (a) exact songs heard in the presentation; (b) new songs; (c) old tunes with new words; (d) new tunes with old words; and (e) old tunes with old words of a different song from the same presentation (`mismatch songs'). Experiment 1 supported the integration hypothesis: Subjects' recognition of components was higher in exact songs (a) than in songs with familiar but mismatched components (e). Melody recognition, in particular, was near chance unless the original words were present. Experiment 2 showed that this integration of melody and text occurred also across different performance renditions of a song and that it could not be eliminated by voluntary attention to the melody. Deux exp\'eriences ont \'et\'e faites pour \'etudier si la repr\'esentation mn\'emonique des chansons consiste en composants ind\'ependants ou int\'egr\'es (m\'elodie ou texte). En pr\'esentent aux sujets une s\'erie d'extraits de chansons peu connues puis un test de reconnaissance. Les sujets doivent reconna\^itre les chansons, les m\'elodies et le texte. Le test comprend cinq types d'items: (a) des chansons entendues lors de la pr\'esentation; (b) des chansons nouvelles; (c) d'anciennes m\'elodies avec de nouveaux mots; (d) de nouvelles m\'elodies avec anciens mots et (e) d'anciennes m\'elodies avec les mots d'une chanson autre que celle de la pr\'esentation: ``chansons qui ne correspondent pas.'' L'exp\'erience appuye l'hypoth\'ese de l'int\'egration: les sujets reconnaissent mieux les composants dans les vraies chansons que dans les chansons avec des composants familier mais qui ne correspondent pas. Ainsi, la reconnaissance de la m\'elodie se fait au hasard sauf lorsque les mots originaux sont pr\'esents. L'exp\'erience 2 montre que cette int\'egration de la m\'elodie et du texte se fait pour des interpr\'etations diff\'erentes d'une chanson et qu'elle ne peut \^etre \'elimin\'ee.}
}

@article{serra2012,
