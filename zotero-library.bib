@article{Pugliesi1992,
  title = {Premenstrual {{Syndrome}} : The {{Medicalization}} of {{Emotion Related To Conflict}} and {{Chronic Role Strain}}},
  author = {Pugliesi, Karen},
  year = {1992},
  journal = {Humboldt Journal of Social Relations},
  volume = {18},
  number = {2, the SOCIOLOGY of EMOTIONS},
  eprint = {23262749},
  eprinttype = {jstor},
  pages = {131--165},
  publisher = {{Department of Sociology, Humboldt State University}},
  issn = {01604341},
  doi = {10.2307/23262749},
  urldate = {2017-10-31},
  abstract = {Premenstrual syndrome (PMS) has become widely accepted as a diagnosis amongs among medical professionals. The incorporation of the concept of PMS in popular culture is evidenced by the proliferation of articles in lay publications and trade books on the topic. Despite the acceptance of this diagnostic label, there is very little known about the etiology of PMS. The symptoms of PMS are numerous and diverse. Many of what are considered the most troubling symptoms involve emotions, with anger, irritability, and depression most common. Recent developments in the sociology of emotions have emphasized the normative nature of emotional experience. Utilizing this theoretical perspective, I propose that deviation from gender differentiated feeling and expression rules leads to the attribution or labeling of PMS. In this context, it is feelings and expressions of anger that are frequently the major concern. Available clinical descriptions and anecdotal evidence suggests that these emotions are often tied to conflicts with partners and/or chronic role strain related to domestic and paid labor. The attribution of PMS occurs through both self-labeling processes and labeling by others. In effect, emotional deviation is explained by the invocation of the concept of PMS. In this way emotional experiences are medicalized. This interpretation of PMS is explored in a content analysis of popular literature from the period 1970 to 1990. Articles about PMS from popular magazines are analyzed with attention to their emphasis on emotional symptoms. 131},
  keywords = {Anger,Emotion,Emotional expression,Gender roles,Menstrual cycle,Mood disorders,Physical symptoms,Premenstrual syndrome,Psychological stress,Symptoms}
}

