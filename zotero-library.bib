@article{peng2013,
  title = {Detecting {{Genetic Association}} of {{Common Human Facial Morphological Variation Using High Density 3D Image Registration}}},
  author = {Peng, Shouneng and Tan, Jingze and Hu, Sile and Zhou, Hang and Guo, Jing and Jin, Li and Tang, Kun},
  editor = {Brem, Rachel B.},
