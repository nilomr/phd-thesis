@article{pereira2011,
  title = {Music and {{Emotions}} in the {{Brain}}: {{Familiarity Matters}}},
  author = {Pereira, Carlos Silva and Teixeira, Jo{\~a}o and Figueiredo, Patr{\'i}cia and Xavier, Jo{\~a}o and Castro, S{\~a}o Lu{\'i}s and Brattico, Elvira},
