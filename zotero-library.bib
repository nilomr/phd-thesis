@incollection{Santamaria2015,
  title = {Premenstrual Experience Premenstrual Syndrome and Dysphoric Disorder},
  booktitle = {Psychopathology in {{Women}}: {{Incorporating Gender Perspective}} into {{Descriptive Psychopathology}}},
