@incollection{Gravholt2013,
  title = {Sex-{{Chromosome Abnormalities}}},
  booktitle = {Emery and {{Rimoin}}'s {{Principles}} and {{Practice}} of {{Medical Genetics}}},
  author = {Gravholt, Claus H.},
  year = {2013},
  pages = {1--32},
  publisher = {{Elsevier}},
  urldate = {2017-12-20},
  abstract = {This article is a revision of the previous edition article by Gail E Graham, Judith E Allanson and Jennifer A Gerritsen, volume 1, pp 1038\textendash 1057, \textcopyright{} 2007, Elsevier Ltd. Sex-chromosome abnormalities are frequent and give rise to two well-defined syndromes, Turner and Klinefelter syndromes, and other less well-defined syndromes such as 47,XXX and 47,XYY syndromes and other conditions with mosaicism, translocations or additional sex chromosomes. The genetic background for these syndromes is incompletely understood, and only one gene on the sex chromosomes, the \{SHOX\} gene, has been unequivocally linked with the syndromes. Other genes are expected to play a role in the different phenotypes presented by the syndromes and await discovery. All syndromes are characterized by delay in diagnosis or even nondiagnosis, increased mortality to a host of different causes, increased morbidity, neurocognitive deficits, but either normal or slightly reduced intelligence quotient. Most females with Turner syndrome do well in society while existing data suggest that persons with Klinefelter, 47,XXX and 47,XYY syndromes do less well. Late diagnosis often leads to postponement of appropriate treatment and diagnosis of complicating traits or disease, such as cardiovascular problems, which could lead to premature death. Specific health issues relate to the individual syndromes, which necessitate a multidisciplinary approach, often including clinical genetics, pediatrics, cardiology, endocrinology, psychology, ENT, and other specialties, adopting a cradle-to-grave philosophy in order to fully envelope the different syndromes.},
  isbn = {978-0-12-383834-6},
  file = {/home/nilomr/Zotero/storage/TMHQUATI/gravholt2013.pdf}
}

