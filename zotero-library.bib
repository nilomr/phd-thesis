@article{Tobias2010,
  title = {Song Divergence by Sensory Drive in Amazonian Birds},
  author = {Tobias, Joseph A. and Aben, Job and Brumfield, Robb T. and Derryberry, Elizabeth P. and Halfwerk, Wouter and Slabbekoorn, Hans and Seddon, Nathalie},
