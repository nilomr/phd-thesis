@article{tuana2006,
  title = {Elisabeth~{{A}}.~ {{Lloyd}}. {{{\emph{The Case}}}}{\emph{ of the }}{{{\emph{Female Orgasm}}}}{\emph{: }}{{{\emph{Bias}}}}{\emph{ in the }}{{{\emph{Science}}}}{\emph{ of }}{{{\emph{Evolution}}}} . 311 Pp., Bibl., Index. {{Cambridge}}, {{Mass}}.: {{Harvard University Press}}, 2005. \$27 (Cloth).},
