@article{ord2012,
  title = {Is Sociality Required for the Evolution of Communicative Complexity? {{Evidence}} Weighed against Alternative Hypotheses in Diverse Taxonomic Groups},
  author = {Ord, Terry J. and {Garcia-Porta}, Joan},
  year = {2012},
