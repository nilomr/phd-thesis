}

@article{carroll1983,
  title = {Visions of the {{Virgin Mary}}: {{The}} Effect of Family Structures on {{Marian}} Apparitions},
  author = {Carroll, Michael P.},
  year = {1983},
  month = sep,
  journal = {Journal for the Scientific Study of Religion},
  volume = {22},
  number = {3},
  eprint = {1385966},
  eprinttype = {jstor},
  pages = {205--221},
  issn = {00218294},
  doi = {10.2307/1385966},
  urldate = {2019-02-03},
  abstract = {Following a brief review of recent literature dealing with the Mary cult, this article focuses on one aspect of that cult: Marian apparitions. Investigation of 50 well-documented apparitions of the Virgin Mary between 1100-1896 and this indicates that 1) Marian apparitions are as likely to occur to males as females; 2) seers are likely to be sexually mature individuals who lack obvious sexual partners; and 3) Italy has always accounted for a disproportionate share of such seers. Following Freud, it is argued that apparitions result from the blocking of sexual impulses. These blocked sexual impulses reactivate infantile impulses long since repressed, and it is these reactivated infantile impulses that determine the content of an apparition. Marian apparitions in particular are produced by the intensification of certain aspects of the Oedipal process. Specifically, males find in the Marian apparitions an ideal way of sublimating their intense sexual desire for the mother, and females find in such apparitions a way of sublimating their sexual desire for the father. This explanation is capable of accounting for all the patterns described previously. However, relative infrequency of Marian apparitions in Spain, which has a family structure similar to that found in Italy, is an anomaly with respect to the interpretation developed here, and that anomaly is discussed. Finally, the interpretation presented here suggests that there should be systematic differences between the Marian apparitions experienced by males and those experienced by females, and those differences do appear in the data.},
  isbn = {0021-8294},
  keywords = {Freud,historie,mu\v{z}i,n\'abo\v{z}enstv\'i,Panna Maria,rodina,sexualita,Sigmund,\v{z}eny,zjeven\'i},
  file = {/home/nilomr/Zotero/storage/XWD9AYAX/1385966.pdf}
}

@book{Carroll1992a,
  title = {The {{Cult}} of the {{Virgin Mary}}: {{Psychological Origins}}},
  author = {Carroll, Michael P.},
  year = {1992},
  publisher = {{Princeton University Press}},
  address = {{Princeton, NJ}},
  abstract = {Tracing devotion to Mary to psychological and historical processes that began in the fifth century, Michael Carroll answers intriguing questions: What explains the many reports of Marian apparitions over the centuries? Why is Mary both "Virgin" and "Mother" simultaneously? Why has the Marian cult always been stronger in certain geographical areas than in others? The first half of the book presents a psychoanalytic explanation for the most salient facts about the Marian cult and the second addresses the question of Marian apparitions".The contention is made that at the root of the Marian cult is the poverty-created father-ineffective family, a family structure in which Oedipal desires in both sons and daughters are intensified.... [A] fascinating and provocative read".--Elizabeth A. Johnson, Journal of Church and State"[Recommended] not only for the wealth of information and the carefully wrought argument that it presents, but also for its potential utility in destroying or at least casting doubt on the idols that prevent us from seeing more clearly and more insightfully the bases of faith and theological conviction".--W. W. Meissner, S.J., M.D., Theological Studies},
  isbn = {0-691-02867-2}
}

