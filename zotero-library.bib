@article{deuster1999,
  title = {Biological, {{Social}}, and {{Behavioral Factors Associated With Premenstrual Syndrome}}},
  author = {Deuster, P. A.},
  year = {1999},
  journal = {Archives of Family Medicine},
  volume = {8},
