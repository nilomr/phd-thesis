@article{alemi2017,
  title = {Afghan {{Refugee Explanatory Models}} of {{Depression}}: {{Exploring Core Cultural Beliefs}} and {{Gender Variations}}},
  author = {Alemi, Qais and Weller, Susan C. and Montgomery, Susanne and James, Sigrid},
  year = {2017},
  month = jun,
  journal = {Medical Anthropology Quarterly},
  volume = {31},
  number = {2},
  pages = {177--197},
  publisher = {{Wiley/Blackwell (10.1111)}},
  issn = {15481387},
  doi = {10.1111/maq.12296},
  urldate = {2018-05-23},
  abstract = {Relatively little empirical attention has been paid to understanding how refugees conceptualize depression and how this concept varies between genders. The purpose of this study was to explore beliefs about depression among Afghans residing in San Diego County, California using Cultural Consensus Analysis. Using the prescribed mixed-method approach, we employed results from in-depth interviews to develop a culturally meaningful questionnaire about depression. Consensus analysis of responses to questionnaire items from 93 Afghans (50 men, 43 women) indicates shared beliefs that associates depression causality with mild traumatic experiences and post-resettlement stressors; symptomatology to include culturally salient idioms of distress; and, treatment selections ranging from lay techniques to professional care. Divergence between genders occurred most in the symptoms subdomain with women associating depression with more somatic items. This study contributes to understanding the etiology of, and cultural responses to depression among this population, which is critical to improving practice with Afghan refugees. This article is protected by copyright. All rights reserved.},
  isbn = {0745-5194 (Print) 0745-5194},
  pmid = {27112930},
  keywords = {Afghan,beliefs,cultural consensus,depression,explanatory model},
  file = {/home/nilomr/Zotero/storage/EZLWGCNK/full-text.pdf}
}

