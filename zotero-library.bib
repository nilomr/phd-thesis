@article{kano2013,
  title = {Head-{{Mounted Eye Tracking}} of a {{Chimpanzee}} under {{Naturalistic Conditions}}},
  author = {Kano, Fumihiro and Tomonaga, Masaki},
  editor = {Warrant, Eric James},
  year = {2013},
