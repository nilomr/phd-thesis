  issn = {10958541},
  doi = {10.1016/S0022-5193(05)80483-7},
  abstract = {Computer simulation was used to examine whether the distribution of song types in a population of birds, and the changes in those present over time, are likely to be influenced by whether or not individuals have repertoires and by the number of neighbours from which a bird can copy. It is predicted from these simulations that having more neighbours will lead to song types lasting for less time in the population, to greater sharing of song types between birds, to song types being shared by spatially separated groups of birds more often, and to a lower proportion of the repertoire being shared between neighbours. Where repertoire sizes are larger the simulations suggest that song types should persist for longer, but they predict little effect on the number of birds sharing a type and no tendency for these birds to split into separate groups. The larger the repertoire the smaller the proportion of it expected to be shared between pairs of neighbours. Comparison with data on the frequency of different songs in a chaffinch population gave a goodness of fit of the simulations to the data that did not vary with repertoire size. However, more simulations with four neighbours matched the data than did ones with two or eight. This number of neighbours was also the most realistic for the population in question. It is concluded that both repertoire size and numbers of neighbours are likely to have a marked impact on various aspects of song distribution in wild populations. \textcopyright{} 1990 Academic Press Limited.},
  file = {/home/nilomr/Zotero/storage/693ABLMB/1-s2.0-S0022519305804837-main.pdf}
}

@article{williams2006,
  title = {Accelerated Evolution of {{Protocadherin11X}}/{{Y}}: {{A}} Candidate Gene-Pair for Cerebral Asymmetry and Language},
  author = {Williams, Nic A. and Close, James P. and Giouzeli, Maria and Crow, Timothy J.},
  year = {2006},
  month = sep,
  journal = {American Journal of Medical Genetics, Part B: Neuropsychiatric Genetics},
  volume = {141},
  number = {6},
  eprint = {16874762},
  eprinttype = {pubmed},
  pages = {623--633},
  issn = {15524841},
  doi = {10.1002/ajmg.b.30357},
  urldate = {2017-10-22},
  abstract = {It has been argued that cerebral asymmetry (the "torque") is the characteristic that defines the human brain and that morphological findings in psychosis are consistent with a deviation in this sex-dependent dimension of brain growth. Evidence from sex chromosome aneuploidies and an association within families between sex and handedness is consistent with the presence of a determinant of cerebral asymmetry (a possible correlate of language) on the X and the Y chromosomes. During hominid evolution a 3.5 Mb translocation occurred from the ancestral X chromosome to the Y chromosome, resulting in duplication of the Protocadherin11X gene, such that it is represented on the X and Y chromosomes in man, whereas there is a single X-linked gene in other mammals. We re-date the duplicative translocation to 6 million years ago, that is, close to the chimpanzee-hominid bifurcation. Sequence comparisons with the chimpanzee, bonobo, gorilla, and orangutan indicate that in contrast to earlier purifying selection there has been accelerated change in the Protocadherin11X ectodomain as well as the Protocadherin11Y sequence in the hominid lineage since the duplication. The evolutionary sequence of events together with the prior case for an X-Y homologous gene suggests that this gene-pair is a candidate for the evolution of hominid-specific characteristics including the sexual dimorphism of cerebral asymmetry, a putative correlate of language.},
  isbn = {1552-4841 (Print)},
  pmid = {16874762},
  keywords = {Cerebral asymmetry,Homo sapiens,Language,Protocadherin11XY,Speciation},
  file = {/home/nilomr/Zotero/storage/6BL7XVPA/Williams et al. - 2006 - Accelerated evolution of Protocadherin11XY A candidate gene-pair for cerebral asymmetry and language.pdf;/home/nilomr/Zotero/storage/FMBX9MK4/williams2006.pdf}
}

@article{williams2009,
  title = {Genetic and Molecular Insights into the Development and Evolution of Sexual Dimorphism},
