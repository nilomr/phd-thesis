}

@article{mortola1990,
  title = {Diagnosis of Premenstrual Syndrome by a Simple, Prospective, and Reliable Instrument: The Calendar of Premenstrual Experiences.},
  author = {Mortola, J F and Girton, L and Beck, L and Yen, S S},
  year = {1990},
